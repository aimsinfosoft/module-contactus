<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
    <table name="aimsinfosoft_contactus" resource="default" engine="innodb" comment="Aimsinfosoft_Contactus">
        <column xsi:type="int" name="id" padding="6" unsigned="false" nullable="false" identity="true" comment="ID"/>
        <column xsi:type="text" name="name" nullable="false" comment="Name"/>
        <column xsi:type="text" name="email" nullable="false" comment="Email"/>
        <column xsi:type="text" name="telephone" nullable="false" comment="Telephone"/>
        <column xsi:type="text" name="comment" nullable="false" comment="Comment"/>
        <column xsi:type="timestamp" name="created_at" nullable="false" comment="Created At"/>
        <constraint xsi:type="primary" referenceId="PRIMARY">
            <column name="id"/>
        </constraint>
    </table>
</schema>